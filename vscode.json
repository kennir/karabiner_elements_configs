{
    "title": "Razer orochi v2 for vscode",
    "rules": [
        {
            "description": "button_4 vscode previous editor",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "pointing_button": "button4"
                    },
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "description": "com.apple.Safari",
                            "bundle_identifiers": [
                                "com.microsoft.VSCode"
                            ]
                        }
                    ],
                    "to": [
                        {
                            "repeat": false,
                            "key_code": "left_arrow",
                            "modifiers": [
                                "left_option",
                                "left_gui"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "button_5 to vscode next editor",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "pointing_button": "button5"
                    },
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "description": "com.apple.Safari",
                            "bundle_identifiers": [
                                "com.microsoft.VSCode"
                            ]
                        }
                    ],
                    "to": [
                        {
                            "repeat": false,
                            "key_code": "right_arrow",
                            "modifiers": [
                                "left_option",
                                "left_gui"
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}