{
  "title": "Open Application",
  "rules": [
    {
      "description": "Hyper+T to Open iTerm2",
      "manipulators": [
        {
          "from": {
            "key_code": "t",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_control",
                "left_shift",
                "left_option"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'iTerm'"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
